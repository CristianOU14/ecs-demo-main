{
  "family": "ecs-demo-app",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::756811050716:role/ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "myapp-container",
      "image": "756811050716.dkr.ecr.us-east-2.amazonaws.com/iue-rep:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}